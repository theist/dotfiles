#!/usr/bin/env ruby

ari = File.open("./.bash_history").readlines.reverse.map{ |x| x.chomp }
ari.map! { |x| x.strip }

newari = []

ari.each do |line|
  next if line =~ /^j /
  next if line =~ /^cd /
  next if line =~ /^cp /
  next if line =~ /^mv /
  next if line =~ /^ack /
  next if line =~ /^sudo shutdown/
  next if line =~ /^ncdu /
  next if line =~ /^vim /
  next if line =~ /^ping /
  next if line =~ /^dig /
  next if line =~ /^host /
  next if line =~ /^git pu/
  next if line =~ /^git ci /
  next if line =~ /^git add /
  next if line =~ /^gcd /
  next if line =~ /^clear /
  next if line =~ /^ls /
  next if line =~ /^rm /
  next if line =~ /^pry/
  next if line =~ /^git co /
  next if line =~ /^sudo apt-get /
  next if line =~ /^docker rm /
  next if line =~ /^docker rmi /
  next if line =~ /^docker kill /
  next if line =~ /^kubectl logs /
  next if line =~ /^kubectl exec /
  next if line =~ /^git revert /
  next if line =~ /^bj release feature /
  next if line =~ /^bj release bugfix /
  next if line =~ /^apt(-get)? /
  next if line =~ /^sudo apt(-get)? /
  next if line =~ /-vxf /
  next if line =~ /-vxzf /
  next if line =~ /-vxjf /
  next if line =~ /^vlc /

  newari.unshift(line) unless newari.index(line)
end

newari.each do |line|
  puts line
end
