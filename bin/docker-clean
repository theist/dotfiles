#!/bin/sh
echo "== Prune containers =="
docker container prune -f

echo "== Prune volumes =="
docker volume prune -f

echo "== Prune networks =="
docker network prune -f

echo "== Prune images =="
docker image prune -f

echo "== Remaining Tagged Images: =="
docker image ls --format "{{.Repository}}:{{.Tag}}"
